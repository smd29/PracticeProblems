class Node:
    def __init__(self,data):
        self.data = data
        self.next = None


#LinkedList class contains Node object
class LinkedList:
    def __init__(self):
        self.head = None

if __name__ == '__main__':
    l_list = LinkedList()
    l_list.head = Node(1)
    second = Node(2)
    third = Node(3)
